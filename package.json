{
  "name": "@adonisjs/redis",
  "version": "3.1.2",
  "description": "AdonisJs addon for Redis",
  "main": "build/providers/RedisProvider.js",
  "files": [
    "build/adonis-typings",
    "build/providers",
    "build/src",
    "build/templates"
  ],
  "types": "build/adonis-typings/redis.d.ts",
  "scripts": {
    "mrm": "mrm --preset=@adonisjs/mrm-preset",
    "pretest": "npm run lint",
    "test": "docker-compose build && docker-compose run --rm tests",
    "lint": "tslint --project tsconfig.json",
    "clean": "del build",
    "copyfiles": "copyfiles \"templates/**/*.txt\" build",
    "compile": "npm run lint && npm run clean && tsc",
    "build": "npm run compile && npm run copyfiles",
    "commit": "git-cz",
    "release": "np",
    "version": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/adonisjs/adonis-redis.git"
  },
  "keywords": [
    "redis",
    "ioredis"
  ],
  "author": "virk,adonisjs",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/adonisjs/adonis-redis/issues"
  },
  "homepage": "https://github.com/adonisjs/adonis-redis#readme",
  "publishConfig": {
    "access": "public",
    "tag": "next"
  },
  "devDependencies": {
    "@adonisjs/application": "^1.0.14",
    "@adonisjs/fold": "^6.1.9",
    "@adonisjs/mrm-preset": "^2.1.0",
    "@adonisjs/sink": "^2.1.11",
    "@types/node": "^12.7.4",
    "commitizen": "^4.0.3",
    "copyfiles": "^2.1.1",
    "cz-conventional-changelog": "^3.0.2",
    "del-cli": "^3.0.0",
    "doctoc": "^1.4.0",
    "husky": "^3.0.5",
    "japa": "^3.0.1",
    "mrm": "^1.2.2",
    "np": "^5.0.3",
    "ts-node": "^8.3.0",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.6.2"
  },
  "nyc": {
    "exclude": [
      "test"
    ],
    "extension": [
      ".ts"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "doctoc README.md --title='## Table of contents' && git add README.md",
      "commit-msg": "node ./node_modules/@adonisjs/mrm-preset/validateCommit/conventional/validate.js"
    }
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "np": {
    "contents": ".",
    "anyBranch": false
  },
  "dependencies": {
    "@poppinss/utils": "^2.0.0",
    "@types/ioredis": "^4.0.17",
    "emittery": "^0.4.1",
    "ioredis": "^4.14.0"
  },
  "peerDependencies": {
    "@adonisjs/core": "2.x.x"
  },
  "adonisjs": {
    "types": "@adonisjs/redis",
    "providers": [
      "@adonisjs/redis"
    ],
    "templates": {
      "config": [
        {
          "src": "config.txt",
          "dest": "redis"
        }
      ],
      "contracts": [
        {
          "src": "contract.txt",
          "dest": "redis"
        }
      ]
    },
    "env": {
      "REDIS_CONNECTION": "local",
      "REDIS_HOST": "127.0.0.1",
      "REDIS_PORT": "6379",
      "REDIS_PASSWORD": ""
    }
  }
}
